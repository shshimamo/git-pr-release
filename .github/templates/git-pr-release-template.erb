[Release] <%= Time.now.localtime("+09:00").strftime("%Y-%m-%d %H:%M:%S") %>

<% milestones = pull_requests.map { |pr| pr.milestone&.title }.uniq %>

<!-- No milestone を最後に移動 -->
<% if milestones.include?(nil) %>
  <% milestones.delete(nil) %>
  <% milestones.shift(nil) %>
<% end %>

<% milestones&.each do |mile_title| -%>
  <%= "## #{mile_title || 'No milestone'}" %>
  <% pull_requests.each do |pr| %>
    <% if pr.milestone&.title == mile_title %>
        <%= "- [#{pr.title}](#{pr.html_url}) by #{pr.mention}" %>
    <% end %>
  <% end %>
<% end -%>
